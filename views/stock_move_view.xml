<odoo>
  <data>
    <record model="ir.ui.view" id="vehicle.stock_move_view">
      <field name="name">Stock Move</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.view_picking_form"/>
      <field name="arch" type="xml">
        <xpath expr="//button[@name='action_open_label_layout']" position="after">
          <button name="enter_vehicle_milage" type="object" string="Enter Vehicle Milage" class="oe_highlight"/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>